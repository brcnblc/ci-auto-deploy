  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Deploy",
      "program": "${workspaceFolder}/deploy.js",
      "cwd": "${workspaceFolder}",
      "args": ["aws myApi1 staging", "--debug", "--simulate", "--passwords", "\"{staging: staging}\""],
      "env": {"CI_AUTO_DEPLOY": ""}
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Deploy All with args",
      "program": "${workspaceFolder}/deploy.js",
      "cwd": "${workspaceFolder}",
      "args": ["aws", "all", "staging", "--debug", "--simulate", "--passwords", "\"{staging: staging}\""],
      "env": {"CI_AUTO_DEPLOY": ""}
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Deploy no Args",
      "program": "${workspaceFolder}/deploy.js",
      "cwd": "${workspaceFolder}",
      "args": ["--debug","--simulate", "--passwords", "\"{staging: staging, production: production}\""],
      "env": {"CI_AUTO_DEPLOY": ""}
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Deploy all no args",
      "program": "${workspaceFolder}/deploy.js",
      "cwd": "${workspaceFolder}",
      "args": ["all", "--debug","--simulate", "--simulate", "--passwords", "\"{staging: staging, production: production}\""],
      "env": {"CI_AUTO_DEPLOY": ""}
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Deploy build",
      "program": "${workspaceFolder}/deploy.js",
      "cwd": "${workspaceFolder}",
      "args": ["aws myApi1 staging", "--build-deploy", "--build-run" ,"--debug", "--passwords", "\"{staging: staging}\""],
      "env": {"CI_AUTO_DEPLOY": ""}
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Deploy production",
      "program": "${workspaceFolder}/deploy.js",
      "cwd": "${workspaceFolder}",
      "args": ["aws myApi1 production" , "--debug", "--passwords", "\"{production: production}\""],
      "env": {"CI_AUTO_DEPLOY": ""}
    }
  ]
}